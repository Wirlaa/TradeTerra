<div class="d-flex mb-2 mx-3">
    <button class="btn btn-outline-light btn-sm ms-auto main-link" style="border:none"><i class="bi bi-arrow-left fs-5"></i></button>
</div>
<form id="loginForm" class="mx-3">
    <div class="mb-3">
        <input type="text" class="form-control" placeholder="Username" autocomplete="username" required id="loginUsername">
    </div>
    <div class="mb-3">
        <input type="password" class="form-control" placeholder="Password" autocomplete="current-password" required id="loginPassword">
    </div>
    <button type="submit" class="btn btn-success w-100">Login</button>
</form>
<div class="text-center mt-3">
    <p>Not a member? <a href="" class="text-white" id="register-link">Register</a></p>
</div>